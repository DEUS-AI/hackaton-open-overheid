AZURE_SERVICEBUS_CONNECTION_STRING="Endpoint=sb://your-namespace.servicebus.windows.net/;SharedAccessKeyName=your-policy;SharedAccessKey=your-key"
AZURE_VALIDATION_QUEUE=your-validation-queue-name-here
AZURE_PII_SCANNING_QUEUE=your-pii-scanning-queue-name-here
AZURE_EXTRACTOR_QUEUE=your-extractor-queue-name-here
AZURE_DATA_STORAGE_QUEUE=your-data-storage-queue-name-here
AZURE_SEARCH_INDEX_QUEUE=your-search-index-queue-name-here
AZURE_NOTIFICATION_QUEUE=your-notification-queue-name-here
AZURE_EMBEDDING_QUEUE=your-embedding-queue-name-here

# Resend email settings
RESEND_API_KEY=your-resend-api-key
NOTIFICATION_EMAIL=your-temp-destination@example.com
# Optional custom from email (must be verified on Resend)
NOTIFICATION_FROM_EMAIL=onboarding@resend.dev
AZURE_DATA_INGESTION_QUEUE=your-notification-queue-name-here
GEMINI_API_KEY=your-gemini-api-here
RESEND_API_KEY=your-resend-api-key-here

# --- MongoDB (local docker-compose defaults) ---
# If MONGO_URI is set, it overrides the separate host/port/user/password values below.
MONGO_URI=
MONGO_HOST=mongodb
MONGO_PORT=27017
MONGO_DB=overheid
MONGO_COLLECTION=messages
MONGO_USER=mongoadmin
MONGO_PASSWORD=mongopass
MONGO_AUTH_DB=admin

# --- LiveKit / Voice Agent ---
LIVEKIT_URL=wss://<project-subdomain>.livekit.cloud
LIVEKIT_API_KEY=<your_livekit_api_key>
LIVEKIT_API_SECRET=<your_livekit_api_secret>

# --- OpenAI ---
OPENAI_API_KEY=sk-...replace...

# --- Neo4j (used by webapp /api/search) ---
NEO4J_URI=neo4j+s://<your-neo4j-host>:7687
NEO4J_USERNAME=<neo4j-username>
NEO4J_PASSWORD=<neo4j-password>